# Enable history
set history save on
# Note that GDBHISTFILE is set in $ZDOTDIR/config/gdb.zsh

# Note the trailing space
set prompt (\033[31mgdb\033[0m) 
set debuginfod enabled off

# Not very nice that there's an explicit path here.
source ~/.config/gdb/int_to_enum.py
source ~/.config/gdb/llvm_prettyprinters.py
